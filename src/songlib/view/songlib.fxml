<GridPane hgap="10" vgap="10" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="songlib.view.songlibController">
   <AnchorPane prefHeight="200.0" prefWidth="200.0" />
   <Button fx:id="addSongButton" mnemonicParsing="false" onAction="#addSong" text="Add Song" />
   <TextField fx:id="songNameField" promptText="Song Name" GridPane.columnIndex="1" />
   <TextField fx:id="songArtistField" promptText="Artist" GridPane.columnIndex="2" />
   <TextField fx:id="songYearField" promptText="Year" GridPane.columnIndex="3" />
