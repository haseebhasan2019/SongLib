   <TextField fx:id="editsongYearField" GridPane.columnIndex="3" GridPane.rowIndex="1" />
   <Button fx:id="editSongButton" mnemonicParsing="false" onAction="#editSong" text="Edit" GridPane.rowIndex="1" />
   <TextField fx:id="editsongAlbumField" GridPane.columnIndex="4" GridPane.rowIndex="1" />
   <Button fx:id="deleteSongButton" layoutX="20.0" layoutY="83.0" mnemonicParsing="false" onAction="#deleteSong" text="Delete" GridPane.columnIndex="1" GridPane.rowIndex="2" />
   <Text fx:id="songDetails" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" wrappingWidth="148.13671875" GridPane.columnIndex="2" GridPane.rowIndex="2" />
